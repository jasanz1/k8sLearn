apiVersion = "apps/v1"
kind       = "Deployment"
metadata  {
  name = "synergychat-crawler"
    labels  {
      app = "synergychat-crawler"
    }
}

spec  {
  replicas = 1
    selector  {
      matchLabels  {
        app = "synergychat-crawler"
      }
    }
  template  {
    metadata  {
      labels  {
        app = "synergychat-crawler"
      }
    }
    spec  {
      containers  {
        new {
          name  = "synergychat-crawler"
            image = "bootdotdev/synergychat-crawler:latest"
            envFrom {
              new {
                    configMapRef {
                      name = "synergychat-crawler-configmap"
                    }
                  }
              }
            }
        }
      }
    }
  }


