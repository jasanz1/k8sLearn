apiVersion = "gateway.networking.k8s.io/v1"
kind = "HTTPRoute"
metadata {
  name = "web-httproute"
}
spec {
  parentRefs {
    new {
      name = "app-gateway"
    }
  }
  hostnames {
    "synchat.internal"
  }
  rules {
    new {
      matches {
        new {
          path {
            type = "PathPrefix"
              value = "/"
          }
        }
      }

      backendRefs {
        new {
          name = "web-service"
            port = 80

        }
      }
    }
  }
}
